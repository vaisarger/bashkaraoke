				player="xplaymidi"
				if [ $(which "${player}" | wc -c ) -eq 0 ]
					then
					[ "${l}" = "IT" ] && echo "Err.: \"${player}\" non trovato..."
					[ "${l}" = "EN" ] && echo "Err.: \"${player}\" not found..."
				fi
				#
				midi="$(echo "${midi}" | sed s\#'^~'#"${HOME}"# )"
				if [ -f "${midi}" ]
					then
					xplaymidi "${midi}" &
					:
				else
					[ "${l}" = "IT" ] && echo "Err.: \"${midi}\" non esiste..."
					[ "${l}" = "EN" ] && echo "Err.: \"${midi}\" doesn't exist..."
				fi
				</code>
			</button>
