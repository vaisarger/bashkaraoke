ouputta_configjs()
	{
	cat <<'configjs_BLOCK'
// JavaScript Document
			$(document).ready( function(){
					   var H =$(window).height();
					   var W =$(window).width();
					  if (H > 771) { H = 770;}
					  
					   $('.textBox').textfill({explicitWidth: W, explicitHeight : H, maxFontPixels: -5});
					
				})		
configjs_BLOCK
	}
#
#
#
